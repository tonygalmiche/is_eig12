<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_ei" model="ir.sequence">
            <field name="name">EI Sequence</field>
            <field name="code">ei.number</field>
            <field name="prefix">EI-</field>
            <field name="padding">4</field>
        </record>

        <record id="seq_eig" model="ir.sequence">
            <field name="name">EIG Sequence</field>
            <field name="code">eig.number</field>
            <field name="prefix">EIG-</field>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
